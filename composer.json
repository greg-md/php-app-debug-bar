{
  "name": "greg-md/php-app-debug-bar",
  "type": "library",
  "description": "Integration of PHP Debug Bar in Greg PHP Application.",
  "keywords": [ "greg-md", "greg-php", "php-app-debug-bar", "greg-app-debug-bar", "app-debug-bar", "debug-bar", "php", "web-artisans" ],
  "homepage": "https://github.com/greg-md/php-app-debug-bar",
  "license": "MIT",
  "authors": [
    { "name": "Grigorii Duca", "email": "contact@greg.md", "homepage": "http://greg.md/" }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3",
    "greg-md/php-app-installer": "dev-master",
    "maximebf/debugbar": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "symfony/var-dumper": "^4"
  },
  "autoload": {
    "psr-4": {
      "Greg\\AppDebugBar\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Greg\\AppDebugBar\\": "tests/"
    }
  }
}
